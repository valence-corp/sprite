{
  "name": "@valence-corp/sprite",
  "version": "0.0.2",
  "description": "A TypeScript driver for ArcadeDB",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "jest ./test/",
    "unit-test": "jest ./test/unit/database/client",
    "integration-test": "jest ./src/test/integration/",
    "dev": "nodemon --watch ./ --ext ts --exec 'rimraf ./dist && tsc && node ./dist/index.js'",
    "build": "rimraf ./dist && tsc -p ./tsconfig.build.json",
    "build-api-docs": "typedoc ./src/api.ts --out ./docs/api --darkHighlightTheme dracula --lightHighlightTheme github-light",
    "build-json-docs": "typedoc ./src/api.ts --json ./docs/_data/docs.json ",
    "build-docs-site": "ts-node ./src/dev/*",
    "build-docs-css": "postcss docs/assets/css/main.css -o docs/assets/css/style.css",
    "exec": "pnpm build && node ./dist/index.js"
  },
  "keywords": [],
  "author": "Austin Pittman",
  "license": "Apache 2.0",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "^9.5.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.14.1"
  }
}