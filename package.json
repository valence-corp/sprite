{
  "name": "@valence-corp/sprite",
  "version": "0.0.2",
  "description": "A TypeScript driver for ArcadeDB",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest ./test/",
    "unit-test": "jest ./test/unit/database/client",
    "integration-test": "jest ./src/test/integration/",
    "dev": "nodemon --watch ./ --ext ts --exec 'rimraf ./dist && tsc && node ./dist/index.js'",
    "build": "rimraf ./dist && tsc -p ./tsconfig.build.json",
    "build-docs": "typedoc ./src/api.ts --out ./docs --darkHighlightTheme dracula --lightHighlightTheme github-light",
    "exec": "pnpm build && node ./dist/index.js"
  },
  "keywords": [],
  "author": "Austin Pittman",
  "license": "Apache 2.0",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.3.3"
  }
}